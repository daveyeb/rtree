{
  "name": "rtree",
  "version": "1.0.0",
  "description": "helper to build wasm binaries",
  "main": "index.js",
  "author": "david yeboah",
  "license": "ISC",
  "scripts": {
    "build:wasm": "wasm-pack build --target web --out-name rtree",
    "build": "npm run build:wasm -- -- --features wasm-bindgen-rayon/no-bundler",
    "copy": "trash /home/thetitan/personal/rtree.rs/public/pkg/* && cp -r ./pkg/* /home/thetitan/personal/rtree.rs/public/pkg/",
    "copy:only": "cp -r ./pkg/* /home/thetitan/personal/rtree.rs/public/pkg/"
  },
  "dependencies": {
    "wasm-pack": "^0.12.1"
  }
}
